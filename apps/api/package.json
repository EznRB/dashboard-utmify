{
  "name": "@utmify/api",
  "version": "0.1.0",
  "description": "Utmify API Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "cd ../../packages/database && pnpm prisma generate",
    "db:push": "cd ../../packages/database && pnpm prisma db push",
    "db:migrate": "cd ../../packages/database && pnpm prisma migrate dev",
    "db:seed": "cd ../../packages/database && pnpm prisma db seed",
    "db:studio": "cd ../../packages/database && pnpm prisma studio",
    "setup:stripe": "tsx src/scripts/setup-stripe.ts"
  },
  "keywords": [
    "utmify",
    "api",
    "fastify",
    "typescript"
  ],
  "author": "Utmify Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/websocket": "^11.2.0",
    "@types/qrcode": "^1.5.5",
    "@types/twilio": "^3.19.3",
    "@utmify/database": "workspace:*",
    "@utmify/shared": "workspace:*",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cron": "^3.1.6",
    "crypto": "^1.0.1",
    "fastify": "^4.25.2",
    "google-ads-api": "^21.0.1",
    "ioredis": "^5.3.2",
    "nanoid": "^5.0.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.8",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "stripe": "^14.12.0",
    "twilio": "^5.9.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.5",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}