        const cspHeader =
        /* istanbul ignore next */
        (cov_27a7le545w().s[27]++, this.generateCSPHeader());
        /* istanbul ignore next */
        cov_27a7le545w().s[28]++;
        if (cspHeader) {
          /* istanbul ignore next */
          cov_27a7le545w().b[11][0]++;
          const headerName =
          /* istanbul ignore next */
          (cov_27a7le545w().s[29]++, this.config.contentSecurityPolicy.reportOnly ?
          /* istanbul ignore next */
          (cov_27a7le545w().b[12][0]++, 'Content-Security-Policy-Report-Only') :
          /* istanbul ignore next */
          (cov_27a7le545w().b[12][1]++, 'Content-Security-Policy'));
          /* istanbul ignore next */
          cov_27a7le545w().s[30]++;
          reply.header(headerName, cspHeader);
        } else
        /* istanbul ignore next */
        {
          cov_27a7le545w().b[11][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[10][1]++;
      }
      // HTTP Strict Transport Security
      cov_27a7le545w().s[31]++;
      if (
      /* istanbul ignore next */
      (cov_27a7le545w().b[14][0]++, this.config.hsts) &&
      /* istanbul ignore next */
      (cov_27a7le545w().b[14][1]++, !this.isDevelopment)) {
        /* istanbul ignore next */
        cov_27a7le545w().b[13][0]++;
        const {
          maxAge,
          includeSubDomains,
          preload
        } =
        /* istanbul ignore next */
        (cov_27a7le545w().s[32]++, this.config.hsts);
        let hstsValue =
        /* istanbul ignore next */
        (cov_27a7le545w().s[33]++, `max-age=${maxAge}`);
        /* istanbul ignore next */
        cov_27a7le545w().s[34]++;
        if (includeSubDomains) {
          /* istanbul ignore next */
          cov_27a7le545w().b[15][0]++;
          cov_27a7le545w().s[35]++;
          hstsValue += '; includeSubDomains';
        } else
        /* istanbul ignore next */
        {
          cov_27a7le545w().b[15][1]++;
        }
        cov_27a7le545w().s[36]++;
        if (preload) {
          /* istanbul ignore next */
          cov_27a7le545w().b[16][0]++;
          cov_27a7le545w().s[37]++;
          hstsValue += '; preload';
        } else
        /* istanbul ignore next */
        {
          cov_27a7le545w().b[16][1]++;
        }
        cov_27a7le545w().s[38]++;
        reply.header('Strict-Transport-Security', hstsValue);
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[13][1]++;
      }
      // X-Frame-Options
      cov_27a7le545w().s[39]++;
      if (this.config.frameOptions) {
        /* istanbul ignore next */
        cov_27a7le545w().b[17][0]++;
        cov_27a7le545w().s[40]++;
        reply.header('X-Frame-Options', this.config.frameOptions);
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[17][1]++;
      }
      // X-Content-Type-Options
      cov_27a7le545w().s[41]++;
      if (this.config.contentTypeOptions) {
        /* istanbul ignore next */
        cov_27a7le545w().b[18][0]++;
        cov_27a7le545w().s[42]++;
        reply.header('X-Content-Type-Options', 'nosniff');
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[18][1]++;
      }
      // Referrer-Policy
      cov_27a7le545w().s[43]++;
      if (this.config.referrerPolicy) {
        /* istanbul ignore next */
        cov_27a7le545w().b[19][0]++;
        cov_27a7le545w().s[44]++;
        reply.header('Referrer-Policy', this.config.referrerPolicy);
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[19][1]++;
      }
      // Permissions-Policy
      cov_27a7le545w().s[45]++;
      if (this.config.permissionsPolicy) {
        /* istanbul ignore next */
        cov_27a7le545w().b[20][0]++;
        const permissionsHeader =
        /* istanbul ignore next */
        (cov_27a7le545w().s[46]++, this.generatePermissionsPolicyHeader());
        /* istanbul ignore next */
        cov_27a7le545w().s[47]++;
        if (permissionsHeader) {
          /* istanbul ignore next */
          cov_27a7le545w().b[21][0]++;
          cov_27a7le545w().s[48]++;
          reply.header('Permissions-Policy', permissionsHeader);
        } else
        /* istanbul ignore next */
        {
          cov_27a7le545w().b[21][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[20][1]++;
      }
      // Cross-Origin policies
      cov_27a7le545w().s[49]++;
      if (this.config.crossOriginEmbedderPolicy) {
        /* istanbul ignore next */
        cov_27a7le545w().b[22][0]++;
        cov_27a7le545w().s[50]++;
        reply.header('Cross-Origin-Embedder-Policy', this.config.crossOriginEmbedderPolicy);
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[22][1]++;
      }
      cov_27a7le545w().s[51]++;
      if (this.config.crossOriginOpenerPolicy) {
        /* istanbul ignore next */
        cov_27a7le545w().b[23][0]++;
        cov_27a7le545w().s[52]++;
        reply.header('Cross-Origin-Opener-Policy', this.config.crossOriginOpenerPolicy);
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[23][1]++;
      }
      cov_27a7le545w().s[53]++;
      if (this.config.crossOriginResourcePolicy) {
        /* istanbul ignore next */
        cov_27a7le545w().b[24][0]++;
        cov_27a7le545w().s[54]++;
        reply.header('Cross-Origin-Resource-Policy', this.config.crossOriginResourcePolicy);
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[24][1]++;
      }
      // Additional security headers
      cov_27a7le545w().s[55]++;
      reply.header('X-DNS-Prefetch-Control', 'off');
      /* istanbul ignore next */
      cov_27a7le545w().s[56]++;
      reply.header('X-Download-Options', 'noopen');
      /* istanbul ignore next */
      cov_27a7le545w().s[57]++;
      reply.header('X-Permitted-Cross-Domain-Policies', 'none');
      /* istanbul ignore next */
      cov_27a7le545w().s[58]++;
      reply.header('X-XSS-Protection', '0'); // Disabled as CSP is more effective
      // Remove server information
      /* istanbul ignore next */
      cov_27a7le545w().s[59]++;
      reply.removeHeader('Server');
      /* istanbul ignore next */
      cov_27a7le545w().s[60]++;
      reply.removeHeader('X-Powered-By');
    } catch (error) {
      /* istanbul ignore next */
      cov_27a7le545w().s[61]++;
      // Log error but don't fail the request
      addBreadcrumb({
        category: 'security',
        message: 'Failed to apply security headers',
        data: {
          error: error.message
        },
        level: 'error'
      });
    }
  }
  // Create middleware function
  createMiddleware() {
    /* istanbul ignore next */
    cov_27a7le545w().f[5]++;
    cov_27a7le545w().s[62]++;
    return async (request, reply) => {
      /* istanbul ignore next */
      cov_27a7le545w().f[6]++;
      cov_27a7le545w().s[63]++;
      this.applyHeaders(reply);
    };
  }
}
// CSP violation reporting endpoint
export async function cspReportHandler(request, reply) {
  /* istanbul ignore next */
  cov_27a7le545w().f[7]++;
  cov_27a7le545w().s[64]++;
  try {
    const report =
    /* istanbul ignore next */
    (cov_27a7le545w().s[65]++, request.body);
    // Log CSP violation
    /* istanbul ignore next */
    cov_27a7le545w().s[66]++;
    addBreadcrumb({
      category: 'security',
      message: 'CSP violation reported',
      data: {
        'blocked-uri': report['csp-report']?.['blocked-uri'],
        'violated-directive': report['csp-report']?.['violated-directive'],
        'original-policy': report['csp-report']?.['original-policy'],
        'document-uri': report['csp-report']?.['document-uri'],
        'referrer': report['csp-report']?.['referrer'],
        'user-agent': request.headers['user-agent'],
        'ip': request.ip
      },
      level: 'warning'
    });
    // You might want to store these reports in a database for analysis
    // await storeCSPViolation(report);
    /* istanbul ignore next */
    cov_27a7le545w().s[67]++;
    reply.code(204).send();
  } catch (error) {
    /* istanbul ignore next */
    cov_27a7le545w().s[68]++;
    reply.code(400).send({
      error: 'Invalid CSP report'
    });
  }
}
// Plugin to register security headers middleware
export async function securityHeadersPlugin(fastify, options =
/* istanbul ignore next */
(cov_27a7le545w().b[25][0]++, {})) {
  /* istanbul ignore next */
  cov_27a7le545w().f[8]++;
  const {
    config,
    isDevelopment =
    /* istanbul ignore next */
    (cov_27a7le545w().b[26][0]++, process.env.NODE_ENV === 'development')
  } =
  /* istanbul ignore next */
  (cov_27a7le545w().s[69]++, options);
  const securityHeaders =
  /* istanbul ignore next */
  (cov_27a7le545w().s[70]++, new SecurityHeaders(config, isDevelopment));
  // Apply security headers to all responses
  /* istanbul ignore next */
  cov_27a7le545w().s[71]++;
  fastify.addHook('onSend', async (request, reply) => {
    /* istanbul ignore next */
    cov_27a7le545w().f[9]++;
    cov_27a7le545w().s[72]++;
    securityHeaders.applyHeaders(reply);
  });
  // Register CSP report endpoint
  /* istanbul ignore next */
  cov_27a7le545w().s[73]++;
  fastify.post('/api/csp-report', {
    schema: {
      body: {
        type: 'object',
        properties: {
          'csp-report': {
            type: 'object',
            properties: {
              'blocked-uri': {
                type: 'string'
              },
              'violated-directive': {
                type: 'string'
              },
              'original-policy': {
                type: 'string'
              },
              'document-uri': {
                type: 'string'
              },
              'referrer': {
                type: 'string'
              }
            }
          }
        }
      }
    }
  }, cspReportHandler);
  // Utility function to get current CSP
  /* istanbul ignore next */
  cov_27a7le545w().s[74]++;
  fastify.decorate('getCSPHeader', () => {
    /* istanbul ignore next */
    cov_27a7le545w().f[10]++;
    cov_27a7le545w().s[75]++;
    return securityHeaders['generateCSPHeader']();
  });
  // Utility function to temporarily modify CSP for specific routes
  /* istanbul ignore next */
  cov_27a7le545w().s[76]++;
  fastify.decorate('withCustomCSP', customDirectives => {
    /* istanbul ignore next */
    cov_27a7le545w().f[11]++;
    cov_27a7le545w().s[77]++;
    return (request, reply) => {
      /* istanbul ignore next */
      cov_27a7le545w().f[12]++;
      const customHeaders =
      /* istanbul ignore next */
      (cov_27a7le545w().s[78]++, new SecurityHeaders({
        contentSecurityPolicy: {
          directives: {
            ...DEFAULT_CONFIG.contentSecurityPolicy.directives,
            ...customDirectives
          }
        }
      }, isDevelopment));
      /* istanbul ignore next */
      cov_27a7le545w().s[79]++;
      customHeaders.applyHeaders(reply);
    };
  });
}
// Utility function to validate CSP directives
export function validateCSPDirectives(directives) {
  /* istanbul ignore next */
  cov_27a7le545w().f[13]++;
  const errors =
  /* istanbul ignore next */
  (cov_27a7le545w().s[80]++, []);
  const validDirectives =
  /* istanbul ignore next */
  (cov_27a7le545w().s[81]++, ['default-src', 'script-src', 'style-src', 'img-src', 'font-src', 'connect-src', 'frame-src', 'worker-src', 'child-src', 'object-src', 'base-uri', 'form-action', 'frame-ancestors', 'upgrade-insecure-requests', 'block-all-mixed-content', 'require-sri-for', 'sandbox']);
  /* istanbul ignore next */
  cov_27a7le545w().s[82]++;
  for (const directive of Object.keys(directives)) {
    /* istanbul ignore next */
    cov_27a7le545w().s[83]++;
    if (!validDirectives.includes(directive)) {
      /* istanbul ignore next */
      cov_27a7le545w().b[27][0]++;
      cov_27a7le545w().s[84]++;
      errors.push(`Invalid CSP directive: ${directive}`);
    } else
    /* istanbul ignore next */
    {
      cov_27a7le545w().b[27][1]++;
    }
  }
  // Check for unsafe directives in production
  /* istanbul ignore next */
  cov_27a7le545w().s[85]++;
  if (process.env.NODE_ENV === 'production') {
    /* istanbul ignore next */
    cov_27a7le545w().b[28][0]++;
    const unsafeDirectives =
    /* istanbul ignore next */
    (cov_27a7le545w().s[86]++, ['unsafe-inline', 'unsafe-eval', 'unsafe-hashes']);
    /* istanbul ignore next */
    cov_27a7le545w().s[87]++;
    for (const [directive, sources] of Object.entries(directives)) {
      /* istanbul ignore next */
      cov_27a7le545w().s[88]++;
      if (Array.isArray(sources)) {
        /* istanbul ignore next */
        cov_27a7le545w().b[29][0]++;
        cov_27a7le545w().s[89]++;
        for (const source of sources) {
          /* istanbul ignore next */
          cov_27a7le545w().s[90]++;
          if (unsafeDirectives.some(unsafe => {
            /* istanbul ignore next */
            cov_27a7le545w().f[14]++;
            cov_27a7le545w().s[91]++;
            return source.includes(unsafe);
          })) {
            /* istanbul ignore next */
            cov_27a7le545w().b[30][0]++;
            cov_27a7le545w().s[92]++;
            errors.push(`Unsafe CSP source in production: ${source} in ${directive}`);
          } else
          /* istanbul ignore next */
          {
            cov_27a7le545w().b[30][1]++;
          }
        }
      } else
      /* istanbul ignore next */
      {
        cov_27a7le545w().b[29][1]++;
      }
    }
  } else
  /* istanbul ignore next */
  {
    cov_27a7le545w().b[28][1]++;
  }
  cov_27a7le545w().s[93]++;
  return {
    valid: errors.length === 0,
    errors
  };
}
// Export configurations and classes
export { SecurityHeaders, DEFAULT_CONFIG, DEVELOPMENT_CONFIG };
export default securityHeadersPlugin;
